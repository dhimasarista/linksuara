<!DOCTYPE html>
<html lang="en">

<head>
    {{> views/partials/head }}
</head>

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        {{> views/partials/sidebar.mustache}}
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                {{> views/partials/topbar.mustache}}
                <!-- End of Topbar -->

                <!-- Begin Page Content -->
                {{> views/contents/inputsuara_content.mustache}}
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            {{> views/partials/footer.mustache}}
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    {{> views//partials/logout_modal.mustache}}
</body>

</html>